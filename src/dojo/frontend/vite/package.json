{
	"name": "dojo-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "concurrently -k -n backend,frontend -c red,cyan \"npm:dev:backend\" \"npm:dev:frontend\"",
		"dev:backend": "cd ../../../.. && uv run uvicorn dojo.core.app:create_app --factory --reload --host 127.0.0.1 --port 8000",
		"dev:frontend": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test:unit": "vitest run",
		"test:e2e": "cd ../../../.. && scripts/run-tests --filter e2e:transactions"
	},
	"dependencies": {
		"@tanstack/query-core": "^5.59.7",
		"@tanstack/vue-query": "^5.59.7",
		"vue": "^3.5.11",
		"vue-router": "^4.4.5"
	},
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@bahmutov/cypress-code-coverage": "^2.7.2",
		"@rollup/pluginutils": "^5.1.0",
		"@vitejs/plugin-vue": "^5.1.2",
		"babel-plugin-istanbul": "^6.1.1",
		"c8": "^9.1.0",
		"concurrently": "^9.1.0",
		"cypress": "^14.5.4",
		"jsdom": "^24.1.0",
		"vite": "^5.4.8",
		"vitest": "^1.6.0"
	}
}
